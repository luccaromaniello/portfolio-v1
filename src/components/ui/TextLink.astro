---
interface Props {
  label: string;
  href: string;
}

const { label, href } = Astro.props;
---

<a
  href={href}
  aria-label={label}
  target="_blank"
  rel="noopener noreferrer"
  class="inline-block text-sm md:text-lg text-primary tracking-wide hover:bg-white hover:text-black"
>
  {label.trim()}
</a>
